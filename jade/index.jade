        link(rel="stylesheet",type="text/css",href="dist/merge.css")
    body
        .navbar.navbar-fixed-top.navbar-inverse
            .navbar-inner
                .container-fluid
                    a.brand(href="https://github.com/sjpsega/beechart-editor",target="_blank") BeeChart-Editor
                    .bee.pull-left
                    ul.nav.pull-right
                        a(href="#chart-type-switch-modal",role="button",data-toggle="modal").btn.btn-inverse#switch 切换图表类型
                        button.btn.btn-inverse#download 下载当前样式
        .container-fluid
            .row-fluid
                .span8.flash-container
                .span4
                    .well#config-container.hide
        include include/chart-type-switch-modal.jade
        include include/loading.jade
        include include/cnzz.jade
                
