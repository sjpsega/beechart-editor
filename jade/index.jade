        script(src="dist/merge.js")
        script(type="text/template")#pie-config
            ul.nav.nav-tabs
                li.active
                    a(href="#general",data-toggle="tab") 通用样式
                li
                    a(href="#pie",data-toggle="tab") 饼图专有
            .tab-content
                .tab-pane.active#general
                    .well#common-set
                        include include/pie/common-pie-set.jade
                    .well#legend-set
                        include include/legend-set.jade
                .tab-pane#pie
                    .well#pie-slice-set
                        include include/pie-slice-set.jade
        script(type="text/template")#line-config
            ul.nav.nav-tabs
                li.active
                    a(href="#general",data-toggle="tab") 通用样式
                li
                    a(href="#line",data-toggle="tab") 折线专有
            .tab-content
                .tab-pane.active#general
                    .well#common-set
                        include include/common-set.jade
                    .well#legend-set
                        include include/legend-set.jade
                    .well#tooltip-set
                        include include/tooltip-set.jade
                    .well#xAxis-set
                        h3 X轴
                        include include/axis-set.jade
                    .well#xAxis-label-set
                        h3 X轴文字
                        include include/xaxis-label-set.jade
                    #for-yAxis-visible
                        .well#yAxis-set
                            h3 Y轴
                            include include/axis-set.jade
                        .well#yAxis-label-set
                            h3 Y轴文字
                            include include/yaxis-label-set.jade
                    .well#canvas-set
                        include include/canvas-set.jade
                .tab-pane#line
                    .well#line-style-set
                        include include/line/line-style-set.jade
                    .well#line-dot-set
                        include include/line/line-dot-set.jade
                    .well#guideLine-set
                        include include/line/guideLine-set.jade
